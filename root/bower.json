{
   "name": "{%= name %}",
   "version": "{%= version %}",
   "description": "{%= description || title %}",
   {% if( licenses.length === 0 ) { %}"private": true,{%
      }
      else if( licenses.length === 1 ) { %}"license": "{%= licenses[0] %}",{%
      }
      else { %}"licenses": [ "{%= licenses.join('", "') %}"{%
      }
   %}
   "dependencies": {
      "requirejs": "2.1.x",
      "requirejs-plugins": "1.0.x",
      "laxar": "LaxarJS/laxar#~1.0.0-alpha.8",
      "laxar_uikit": "LaxarJS/laxar_uikit#~1.0.0-alpha.5",
      "laxar_patterns": "LaxarJS/laxar_patterns#~1.0.0-alpha.0"
   },
   "devDependencies": {
      "jasmine": "1.3.x"
   }
}
